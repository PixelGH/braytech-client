{
    "devmode": {
        "displayName": "Developer Features",
        "type": "switch",
        "default": false
    },
    "braytechBeta": {
        "displayName": "Use beta.braytech.org",
        "type": "switch",
        "default": false
    },
    "clientTheme": {
        "displayName": "Client Theme",
        "type": "pick",
        "options": [
            { "display": "Light", "value": "light" },
            { "display": "Dark", "value": "dark" }
        ],
        "requiresCSSUpdate": true,
        "default": "light"
    },
    "tray": {
        "displayName": "Close to tray",
        "type": "switch",
        "default": true
    }
}
